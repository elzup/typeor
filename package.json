{
  "name": "typeor",
  "version": "1.2.0",
  "author": "elzup",
  "repository": "elzup/typeor",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "eslint": "7.4.0",
    "eslint-config-nzap": "3.2.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "lint-staged": {
    "scripts/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
