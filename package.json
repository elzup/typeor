{
  "name": "typeor",
  "version": "1.2.0",
  "author": "elzup",
  "repository": "elzup/typeor",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "14.17.1",
    "eslint": "7.27.0",
    "eslint-config-nzap": "4.3.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.3.0",
    "ts-jest": "26.5.6",
