    "@types/node": "12.7.12",
    "eslint": "6.5.1",
    "eslint-config-nzap": "^1.1.0",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
