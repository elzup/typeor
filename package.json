{
  "name": "typeor",
  "version": "1.2.0",
  "author": "elzup",
  "repository": "elzup/typeor",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "14.17.34",
    "eslint": "8.3.0",
    "eslint-config-nzap": "5.0.0",
    "husky": "7.0.4",
    "jest": "27.4.2",
    "lint-staged": "11.2.6",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "scripts/**/*.ts": [
