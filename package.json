{
  "name": "typeor",
  "version": "1.2.0",
  "author": "elzup",
  "repository": "elzup/typeor",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "eslint": "7.12.1",
    "eslint-config-nzap": "3.2.0",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "ts-jest": "26.4.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "lint-staged": {
    "scripts/**/*.ts": [
