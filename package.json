{
  "name": "typeor",
  "version": "1.2.0",
  "author": "elzup",
  "repository": "elzup/typeor",
  "dependencies": {},
  "devDependencies": {
    "@elzup/tsconfig": "^1.0.1",
    "@swc/core": "1.2.207",
    "@swc/jest": "^0.2.20",
    "@types/jest": "27.5.2",
    "@types/node": "17.0.30",
    "eslint": "8.14.0",
    "eslint-config-nzap": "5.3.0",
    "husky": "7.0.4",
    "jest": "28.0.3",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
