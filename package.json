{
  "name": "typeor",
  "version": "1.1.1",
  "author": "elzup",
  "repository": "elzup/typeor",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/node": "12.12.3",
    "eslint": "7.2.0",
    "eslint-config-nzap": "2.1.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.7",
    "prettier": "2.0.5",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.3"
  },
  "lint-staged": {
    "scripts/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
